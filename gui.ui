<?xml version='1.0' encoding='utf-8'?>
<interface>
  <object class="ttk.Frame" id="mainwindow">
    <property name="height">200</property>
    <property name="padding">0 0 0 10</property>
    <property name="width">200</property>
    <layout>
      <property name="column">0</property>
      <property name="propagate">True</property>
      <property name="row">0</property>
      <property name="sticky">nesw</property>
      <columns>
        <column id="0">
          <property name="pad">10</property>
        </column>
        <column id="1">
          <property name="pad">10</property>
        </column>
        <column id="2">
          <property name="pad">10</property>
        </column>
      </columns>
    </layout>
    <child>
      <object class="ttk.Label" id="lmain">
        <property name="anchor">center</property>
        <property name="font">sans 12 bold</property>
        <property name="padding">0 0 0 10</property>
        <property name="text" translatable="yes">Command Properties</property>
        <layout>
          <property name="column">0</property>
          <property name="columnspan">3</property>
          <property name="propagate">True</property>
          <property name="row">0</property>
          <property name="sticky">ew</property>
        </layout>
      </object>
    </child>
    <child>
      <object class="ttk.Button" id="Button_1">
        <property name="command">on_button1_clicked</property>
        <property name="text" translatable="yes">Set Template</property>
        <layout>
          <property name="column">0</property>
          <property name="propagate">True</property>
          <property name="row">1</property>
        </layout>
      </object>
    </child>
    <child>
      <object class="ttk.Button" id="btnMonitor">
        <property name="command">toggle_monitor</property>
        <property name="text" translatable="yes">Start Monitoring</property>
        <layout>
          <property name="column">1</property>
          <property name="propagate">True</property>
          <property name="row">1</property>
        </layout>
      </object>
    </child>
    <child>
      <object class="ttk.Button" id="Button_3">
        <property name="command">on_button3_clicked</property>
        <property name="text" translatable="yes">Button 3</property>
        <layout>
          <property name="column">2</property>
          <property name="propagate">True</property>
          <property name="row">1</property>
        </layout>
      </object>
    </child>
  </object>
</interface>
